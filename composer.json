{
    "name": "melifaro/melifaro-booking-bundle",
    "type": "symfony-bundle",
    "keywords": ["booking", "availability", "calendar"],
    "homepage": "http://me1ifaro.github.io/MelifaroBookingBundle",
    "description": "Adds some booking functionality to your Symfony 2 project",
    "license": "MIT",
    "authors": [
        {
            "name": "Iakov Mishchenko",
            "email": "iakov@codeforges.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":">=5.3.2",
        "symfony/finder": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/options-resolver": "~2.3",
        "symfony/framework-bundle": "~2.3",
        "twig/twig": "~1.5",
        "twig/extensions": "~1.0",
        "symfony/browser-kit": "2.4.*@dev",
        "doctrine/orm": ">=2.2,<2.5-dev",
        "doctrine/doctrine-bundle": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "doctrine/doctrine-fixtures-bundle": "dev-master"
    },
    "suggest": {
        "doctrine/mongodb-odm": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Melifaro\\BookingBundle": "" }
    },
    "target-dir": "Melifaro/BookingBundle"
}
